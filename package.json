{
  "name": "image-texter",
  "version": "0.1.2",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "npm run clean && webpack -p",
    "clean": "rm -rf dist",
    "deploy": "gh-pages -d dist",
    "bump": "npm version patch",
    "cypress": "cypress run --reporter junit --reporter-options \"mochaFile=build/cypress-results.xml,toConsole=true\"",
    "cypress:open": "cypress open",
    "start:test_server": "node ./cypress/server",
    "cypress:buildserver": "start-server-and-test start:test_server http://localhost:8087 cypress",
    "cypress:open:buildserver": "start-server-and-test start:test_server http://localhost:8087 cypress-open"
  },
  "dependencies": {
    "p5": "^0.5.9"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cypress": "^3.1.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^2.28.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
